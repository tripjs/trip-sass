{
  "name": "trip-sass",
  "description": "Compiles Sass to CSS",
  "version": "0.0.1",
  "author": "Callum Locke",
  "babel": {
    "presets": [
      "es2015-node4"
    ]
  },
  "dependencies": {
    "bluebird": "^3.1.1",
    "code-error": "0.0.1",
    "lazy-builder": "^0.0.3",
    "lodash": "^3.10.1",
    "micromatch": "^2.3.7",
    "node-sass": "^3.4.2",
    "sander": "^0.4.0",
    "subdir": "^0.0.3"
  },
  "devDependencies": {
    "babel-cli": "^6.4.0",
    "babel-eslint": "^5.0.0-beta6",
    "babel-preset-es2015-node4": "^2.0.2",
    "eslint": "^1.10.3",
    "eslint-config-trip": "^1.0.1",
    "eslint-plugin-babel": "^3.0.0",
    "rimraf": "^2.5.0"
  },
  "eslintConfig": {
    "extends": "trip"
  },
  "files": [
    "dist"
  ],
  "jsnext:main": "src/index.js",
  "license": "MIT",
  "main": "dist/index.js",
  "repository": "tripjs/trip-sass",
  "scripts": {
    "build": "rimraf dist && babel src --out-dir dist --source-maps",
    "buildwatch": "rimraf dist && babel src --out-dir dist --source-maps --watch",
    "prepublish": "npm run build",
    "test": "eslint ."
  }
}
